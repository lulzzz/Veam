<div class="mvc-lookup-overlay">
    <div class="mvc-lookup-wrapper" style="font-size: 1.5em !important;">
        <div id="MvcLookupDialog" class="mvc-lookup-dialog glyphicon-modal-window">
            <div class="mvc-lookup-header" style="background-color: #357ca5 !important">
                <span class="mvc-lookup-title"></span>
                <button class="mvc-lookup-close">×</button>
            </div>
            <div class="mvc-lookup-body modal-body">
                
                <div class="mvc-lookup-search-area">
                    <input class="mvc-lookup-search" />
                    <input class="mvc-lookup-rows" />
                    <button class="mvc-lookup-selector"></button>
                </div>
                <div class="mvc-lookup-content">
                    <div class="mvc-lookup-dialog-loader">
                        <span></span>
                    </div>
                    <div class="mvc-lookup-dialog-error">
                    </div>
                    <div class="box-body no-padding">
                        <div class="mvc-lookup-table table-responsive mailbox-messages">
                            <table>
                                <thead></thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                    <div class="mvc-lookup-footer">
                        <button type="button" class="mvc-lookup-load-more"></button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>